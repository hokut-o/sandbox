.wrap
	width: 100vw
	height: 100vh
	display: flex
	justify-content: center
	align-items: center
input
	margin-left: 20px
#email
	-webkit-appearance: none
	border: 1px solid #D9D9D9
	border-radius: 0
	box-shadow: inset 0 2px 0 0 rgba(0, 0, 0, .05)
	height: 40px
	padding: 0 10px
	background: white
	width: 100%
#email:focus
	outline: none
	background: #EDF6FA
#placement
	position: relative
ul
	background: #FFFFFF
	border: 1px solid #D9D9D9
	box-shadow: 0 2px 0 0 rgba(0, 0, 0, .05)
	margin-top: -1px
	position: absolute
	top: 40px
	left: 0
	width: 100%
li
	padding: 0 10px
.highlighted
	background-color: #F0F0F0







