-
	var domain = './'
	var imgSrc = './img/'
	var description = 'foo'
	var keywords = 'foo'
	var title = 'foo'
	var date = +new Date()

doctype html
html(lang='ja')
	head
		include include/_meta
		include include/_ogp
		include include/_title
		include include/_css
	body
		nav.navbar.navbar-expand-md.navbar-light.bg-light.sticky-top
			.container-fluid
				a.navbar-brand(href='#')
					img(src='img/logo.png')
				button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarResponsive')
					span.navbar-toggler-icon
				#navbarResponsive.collapse.navbar-collapse
					ul.navbar-nav.ml-auto
						li.nav-item.active
							a.nav-link(href='#') Home
						li.nav-item
							a.nav-link(href='#') About
						li.nav-item
							a.nav-link(href='#') Services
						li.nav-item
							a.nav-link(href='#') Team
						li.nav-item
							a.nav-link(href='#') Connect

		include include/_js